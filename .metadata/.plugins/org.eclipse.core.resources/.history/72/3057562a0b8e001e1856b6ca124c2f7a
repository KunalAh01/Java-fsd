package com;


class Bike1{
	int x=9;

	 Bike1() {
		System.out.println("Bike is created");
		System.out.println(x);
	}
	 
}
class Bike2{
	int id;
	int name;
	public Bike2("id and name are "+ id , name);
}
public class SimpleConstructor {
		public static void main(String[] args) {
		Bike1 b1= new Bike1();
		
		
		

	}

}
